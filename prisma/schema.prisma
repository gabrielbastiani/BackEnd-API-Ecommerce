  filtercategories FilterCategory[]
  filterattributes FilterAttribute[]

  created_at DateTime? @default(now()) @db.Timestamptz(3)
  updated_at DateTime? @updatedAt

  store    Store?  @relation(fields: [store_id], references: [id])
