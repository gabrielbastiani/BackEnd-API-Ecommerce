  image        String?
  order        Int?
  position     String?           @db.VarChar(300)
  slugPosition String?           @db.VarChar(295)
  status       StatusSocialMedia @default(Disponivel)

  created_at DateTime? @default(now()) @db.Timestamptz(3)
